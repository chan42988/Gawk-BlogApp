<div id='ucontainer'>
<br>
<h1>Sign up</h1>

<%= form_for User.new do |f| %>
  <%=f.label :username %>
  <%=f.text_field :username %><br><br>
  <%=f.label :password %>
  <%=f.password_field :password %><br><br>
  <%=f.label :email %>
  <%=f.text_field :email %><br><br>
  <%=f.submit %><br><br>

<% end %><hr>