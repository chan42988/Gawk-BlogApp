<div id='econtainer'>
<br>
<%= form_for User.new do |f| %>
	<%=f.label :username %>
	<%=f.text_field :username %><br><br>
	<%=f.label :password %>
	<%=f.text_field :password %><br><br>
	<%=f.label :email %>
	<%=f.text_field :email %><br><br>
	<%=f.submit "Update" %>
<% end %>
<p>or</p>
<%= link_to "Delete Account", home_index_path, method: :delete %>
</div>